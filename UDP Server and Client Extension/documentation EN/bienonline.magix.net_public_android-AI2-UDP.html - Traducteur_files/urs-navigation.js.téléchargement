function generateMainMenuEntry(label, link, subMenu)
{ htmlcode  = "<ul class='Navigation'>"
  htmlcode += "  <li class='HauptPunkt'><a href='"+ link + "'>" + label + "</a></li>"
  htmlcode += subMenu;
  htmlcode += "</ul>"

  return htmlcode;
}

function generateMenu(menuClass)
{ htmlcode = ""
   + "<small>&nbsp;&Uuml;ber Anregungen und Fragen freue <br>&nbsp;ich mich: "
   +"<a href='&#109;&#097;&#105;&#108;&#116;&#111;&#058;&#098;&#105;&#101;&#110;&#111;&#110;&#108;&#105;&#110;&#101;&#064;&#103;&#109;&#120;&#046;&#100;&#101;'>"
   +"E-Mail an Ulli</a>"
   +"</small><br><br>"
+ generateMainMenuEntry("NXT",             "nxt.html",          (menuClass  == "nxt")?           nxtMenu : "")
+ generateMainMenuEntry("I&sup2;C",        "i2c.html",          (menuClass  == "i2c")?           i2cMenu : "")
+ generateMainMenuEntry("DotNet",          "dotnet.html",       (menuClass  == "dotnet")?        DotNetMenu : "")
+ generateMainMenuEntry("AVR",             "avr.html",          (menuClass  == "avr")?           avrMenu : "")
+ generateMainMenuEntry("Arduino/ESP8266", "arduino.html",      (menuClass  == "arduino")?       arduinoMenu : "")
+ generateMainMenuEntry("Android",         "android.html",      (menuClass  == "android")?       androidMenu : "")
+ generateMainMenuEntry("Raspberry Pi",    "rasp.html",         (menuClass  == "rasp")?          raspMenu : "")
+ generateMainMenuEntry("USB",             "usb.html",          (menuClass  == "usb")?           usbMenu : "")
+ generateMainMenuEntry("Projekte",        "projekte.html",     (menuClass  == "projekte")?      projekteMenu: "")
+ generateMainMenuEntry("3D-Druck",        "3D.html",           (menuClass  == "druck3d")?       druckMenu: "")
+ generateMainMenuEntry("Ideen",           "ideen.html",        (menuClass  == "ideen")?         ideenMenu: "")
+ generateMainMenuEntry("... und sonst",   "undsonst.html",     (menuClass  == "undsonst")?      undsonstMenu: "")
+ generateMainMenuEntry("Gescheitertes",   "gescheitert.html",  (menuClass  == "gescheitertes")? gescheitertesMenu : "")
;

return htmlcode;
}

